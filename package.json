{
	"name": "obsidian-incremental-reading",
	"version": "0.1.0",
	"description": "Spaced repetition for reading notes in Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"dev:link": "ln -s \"$PWD\" \"$npm_config_vault/.obsidian/plugins/incremental-reading\"",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write .",
		"test": "jest",
		"test:unit": "jest",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prepare": "husky"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"template"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.19.19",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^16.1.5",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"ts-jest": "^29.1.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^3.2.4"
	},
	"lint-staged": {
		"*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"ts-fsrs": "^5.2.3"
	}
}
